{
    "name": "functions",
    "description": "Cloud Functions for Firebase",
    "scripts": {
        "logs": "firebase functions:log",
        "start": "firebase emulators:start",
        "config": "firebase functions:config:get > .runtimeconfig.json",
        "test_crawling": "mocha test/crawling/*.js -t 10000",
        "test_profile": "mocha test/router/setting/input/profile_spec -t 10000 && mocha test/router/setting/input/credit_spec -t 10000",
        "test": "mocha test/router/public/service_spec -t 10000",
        "lint": "eslint . --fix"
    },
    "engines": {
        "node": "16"
    },
    "main": "index.js",
    "dependencies": {
        "axios": "^0.25.0",
        "cheerio": "^1.0.0-rc.10",
        "crypto-js": "^4.1.1",
        "express": "^4.18.1",
        "firebase-admin": "^9.8.0",
        "firebase-functions": "^3.14.1",
        "puppeteer": "^13.2.0"
    },
    "devDependencies": {
        "chai": "^4.3.4",
        "eslint": "^8.20.0",
        "firebase-functions-test": "^0.2.0",
        "mocha": "^9.1.4",
        "supertest": "^6.2.2"
    },
    "private": true
}